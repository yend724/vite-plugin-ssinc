{
  "name": "vite-plugin-ssinc",
  "version": "1.0.10",
  "description": "The plugin for including SSI-like includes in Vite.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:ts": "tsup",
    "prebuild": "rm -rf ./dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yend724/vite-plugin-ssinc.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vite",
    "vite-plugin",
    "vite-plugin-ssinc",
    "ssi"
  ],
  "author": "YEND",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yend724/vite-plugin-ssinc/issues"
  },
  "homepage": "https://github.com/yend724/vite-plugin-ssinc#readme",
  "devDependencies": {
    "@types/node": "^22.5.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}
